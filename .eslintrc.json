{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "semi": ["error", "always"],
      "curly": ["error", "all"],
      "comma-dangle": "off"
    }
}
